{
  "name": "daily-bible-server",
  "version": "1.0.0",
  "main": "server.ts",
  "scripts": {
    "start:dev": "env-cmd -f .env.dev ts-node-dev --watch .env.dev --require tsconfig-paths/register --transpile-only --respawn --inspect=4321 src/server.ts",
    "seed:dev": "env-cmd -f .env.dev ts-node-dev --require tsconfig-paths/register --transpile-only src/Seed/MockData/importDataToDB.ts",
    "build:js": "rimraf build && tsc --noEmit false && tsc-alias && xcopy .env.dev build &&",
    "lint": "npx tsc && npx prettier --write --cache --log-level error . && npx cspell --cache --no-progress ./**/* && npx eslint --fix .",
    "merge": "git pull origin main && git merge main"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "express": "^4.19.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.4",
    "mongoose-auto-number": "^1.0.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "pug": "^3.0.2",
    "rimraf": "^5.0.5",
    "sharp": "^0.33.3",
    "validator": "^13.12.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@cspell/dict-he": "^4.0.0",
    "@cspell/eslint-plugin": "^8.8.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/csurf": "^1.11.5",
    "@types/express": "^4.17.21",
    "@types/hpp": "^0.2.6",
    "@types/html-to-text": "^9.0.4",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.11",
    "@types/nodemailer": "^6.4.15",
    "@types/pug": "^2.0.10",
    "@types/sharp": "^0.31.1",
    "@types/validator": "^13.11.9",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "cspell": "^8.8.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.9",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": "^20.12.0"
  }
}
