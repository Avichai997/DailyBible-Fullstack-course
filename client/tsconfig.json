{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": false,
    "baseUrl": "./",
    "skipLibCheck": true,
    "esModuleInterop": false,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "outDir": "build",
    "types": ["vite/client", "vitest/importMeta", "vitest/globals"],
    "paths": {
      "@public/*": ["public/*"],
      "@Assets/*": ["src/Assets/*"],
      "@Components/*": ["src/Components/*"],
      "@Hooks/*": ["src/Hooks/*"],
      "@Pages/*": ["src/Pages/*"],
      "@Utils/*": ["src/Utils/*"],
      "@ApiService/*": ["src/ApiService/*"],
      "@RequestApi/*": ["src/ApiService/RequestApi/*"],
      "@Interfaces/*": ["src/ApiService/Interfaces/*"],
      "@Tests/*": ["src/ApiService/Tests/*"],
      "@Common/*": ["src/Common/*"],
      "@Atoms/*": ["src/Atoms/*"],
      "@CommonComponents/*": ["src/Components/Common/*"],
      "@CommonConstants": ["src/Common/CommonConstants"],
      "@CommonFunctions": ["src/Common/CommonFunctions"],
      "@CommonInterfaces": ["src/Common/CommonInterfaces"],
      "@Components/Common/*": ["noSuchDirectory"],
      "@Common/CommonConstants": ["noSuchDirectory"],
      "@Common/CommonFunctions": ["noSuchDirectory"],
      "@Common/CommonInterfaces": ["noSuchDirectory"]
    }
  },
  "include": ["vite.config.ts", "vitest.config.ts", ".eslintrc.cjs", "src", "package.json"]
}
