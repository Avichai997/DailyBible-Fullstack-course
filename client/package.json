{
  "name": "project-client",
  "private": true,
  "version": "1.0.5",
  "type": "module",
  "scripts": {
    "prepare": "cd.. && husky install && cd .husky/_ && del /f .gitignore",
    "start:dev": "vite --mode development",
    "start:prod": "vite --mode prod",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "test:watch": "vitest",
    "testOnce": "vitest --run",
    "test:ui": "vitest --ui",
    "preview:dev": "vite preview --mode development",
    "preview:prod": "vite preview --mode production",
    "merge": "git pull && git pull origin Development && git merge Development",
    "lint": "tsc && prettier --write --cache --log-level silent . && cspell --cache --no-progress ../**/* && eslint --fix --cache .",
    "lint:parallel": "concurrently \"cd ../server && npm run lint\" \"npm run lint\""
  },
  "dependencies": {
