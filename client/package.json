    "@tanstack/react-query-devtools": "^4.29.6",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.4.0",
    "dayjs": "^1.11.10",
    "deep-object-diff": "^1.1.9",
    "formik": "^2.2.9",
    "framer-motion": "^11.0.25",
    "jotai": "^2.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.4",
    "react-router-dom": "^6.11.1",
    "react-toastify": "^9.1.2",
    "sass": "^1.62.1",
    "stylis": "^4.2.0",
